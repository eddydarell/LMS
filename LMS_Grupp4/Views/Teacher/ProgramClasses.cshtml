@model IEnumerable<LMS_Grupp4.Models.LMS_Models.ProgramClass>

@{
    ViewBag.Title = "ProgramClasses";
}

<h2>My Programs</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ClassName)
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(programClass => item.ClassName)
            </td>

        </tr>
    }

</table>
