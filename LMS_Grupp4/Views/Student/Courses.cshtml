@model IEnumerable<LMS_Grupp4.Models.LMS_Models.Course>
@using Microsoft.AspNet.Identity

@{
    ViewBag.Title = "ProgramClasses";
}

@Html.ActionLink("Add new Course", "AddCourse", new { id = User.Identity.GetUserId() })

<h2>My Programs</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CourseName)
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(course => item.CourseName)
            </td>

        </tr>
    }

</table>

